<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>MyVelocityServlet</servlet-name>
    <servlet-class>com.test.MyVelocityViewServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MyLayoutServlet</servlet-name>
    <servlet-class>com.test.MyLayoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>MyVelocityServlet</servlet-name>
  <url-pattern>/servlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MyLayoutServlet</servlet-name>
    <url-pattern>/layout</url-pattern>
  </servlet-mapping>
</web-app>
